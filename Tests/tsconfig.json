{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noLib": false,
    "sourceMap": true,
    "pretty": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "typeRoots": [
      "../node_modules/@types",
      "../node_modules"
    ],
    "types": [
      "node"
    ]
  },
  "exclude": [
    "./../node_modules",
    "./../obj",
    "./../dist",
    "./../src"
  ],
  "compileOnSave": true
}
